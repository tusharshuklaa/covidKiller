{
  "svg.preview.background": "transparent",
  "editor.fontSize": 18,
  "editor.lineHeight": 30,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    }
  },
  "editor.rulers": [
    80,
    130,
    140,
  ],
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#59e5ae",
    "activityBar.activeBorder": "#ae5ae5",
    "activityBar.background": "#59e5ae",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#ae5ae5",
    "activityBarBadge.foreground": "#15202b",
    "statusBar.background": "#2dde98",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#1dbb7c",
    "tab.activeBorder": "#59e5ae",
    "titleBar.activeBackground": "#2dde98",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#2dde9899",
    "titleBar.inactiveForeground": "#15202b99"
  },
  "peacock.color": "#2dde98",
}